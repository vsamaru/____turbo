{
  "npmClient": "pnpm",
  "pipeline": {
    "dev": {
      "cache": false,
      "outputs": [
        "dist/**",
        "build/**"
      ]
    },
    "build": {
      "outputs": [
        "dist",
        "./packages"
      ]
    },
    "test": {
      "outputs": [
        "dist/**",
        "build/**"
      ]
    }
  }
}
